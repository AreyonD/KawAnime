{
  "presets": [
    ["env", { "modules": false }],
    "stage-2"
  ],
  "plugins": [
    "add-filehash",
    ["transform-imports", {
        "vuetify": {
            "transform": "vuetify/src/components/${member}"
            // "preventFullImport": true  // waiting a-la-carte
        }
    }],
    "transform-runtime"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul", "syntax-dynamic-import"]
    }
  }
}
